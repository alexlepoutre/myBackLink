{% extends 'base.html.twig' %}

{% block title %}BackLinkLog{% endblock %}

{% block body %}
    <h1>BackLinkLog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ back_link_log.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ back_link_log.createdAt ? back_link_log.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FoundIt</th>
                <td>{{ back_link_log.foundIt ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>LogText</th>
                <td>{{ back_link_log.logText }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('back_link_log_index') }}">back to list</a>

    <a href="{{ path('back_link_log_edit', {'id': back_link_log.id}) }}">edit</a>

    {{ include('back_link_log/_delete_form.html.twig') }}
{% endblock %}
